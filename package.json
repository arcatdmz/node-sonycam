{
  "name": "node-sonycam",
  "version": "0.0.6",
  "description": "Node.js library to fetch images from Sony DSLRs (e.g., ILCE-QX1) written in TypeScript",
  "type": "module",
  "types": "dist/index.d.ts",
  "main": "dist/index.mjs",
  "module": "dist/index.mjs",
  "scripts": {
    "build": "rollup -c",
    "clean": "run-p clean:*",
    "clean:docs": "del-cli docs/out",
    "clean:lib": "del-cli dist",
    "prettify": "prettier --write ./**/*.{ts,tsx}",
    "typedoc": "typedoc --out ./docs ./lib/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arcatdmz/node-sonycam.git"
  },
  "keywords": [
    "nodejs",
    "sony-alpha-cameras"
  ],
  "author": "Jun Kato <i@junkato.jp>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/arcatdmz/node-sonycam/issues"
  },
  "homepage": "https://github.com/arcatdmz/node-sonycam#readme",
  "dependencies": {
    "fast-xml-parser": "^4.0.7",
    "node-fetch": "^3.2.3",
    "semver": "^7.3.7"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.3.2",
    "@types/node": "^17.0.26",
    "@types/semver": "^7.3.9",
    "del-cli": "^4.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "rollup": "^2.70.2",
    "typedoc": "^0.22.15",
    "typescript": "^4.6.3"
  }
}
